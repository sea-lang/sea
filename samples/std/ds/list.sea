use std/str
use std/ds/list

fun main(): int {
	var list = make_list(2, sizeof(String), 2.0f)

	printf(c"List cap is %d\n", list.cap)

	let characters = [
		"Gandalf",
		"Saruman",
		"Radagast",
		"Sauron",
		"Beorn",
		"Bilbo",
		"Frodo",
		"Sam",
		"Pippin",
		"Merry"
	]

	for i in 0 to 10 -> list_append(ref list, ref characters[i])

	printf(c"List cap is %d\n", list.cap)

	for i in 0 to list.len {
		println(list_get(ref list, i) as ^String^)
	}

	ret 0
}
